{
    "manifest_version": 3,
    "name": "Sidebar Extension",
    "version": "1.0",
    "description": "Add a sidebar to all web pages",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["scripts/content.js"],
        "run_at": "document_end"
      }
    ]
}
  