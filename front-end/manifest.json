{
  "name": "Bookmark Copilot",
  "version": "0.1",
  "manifest_version": 3,
  "description": "AI helps you organize your bookmarks.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["sidePanel", "contextMenus","bookmarks","favicon","history"],
  "host_permissions": [
    "https://asia-northeast1-bookmarkai-414803.cloudfunctions.net/*"
  ],
  "action": {
    "default_title": "Open Side Panel"
  }
  
}
