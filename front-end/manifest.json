{
  "manifest_version": 3,
  "name": "Bookmark Side Panel",
  "version": "1.0",
  "permissions": ["bookmarks", "sidePanel", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Toggle Side Panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}
